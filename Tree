public abstract class Tree {
    protected Node root;

    //Construtor:
    public Tree(Node root) {
        this.root = root;
    }

    public abstract void addBT(int key, Node atual);
    public abstract Node addBTS(int key, Node atual);
    public abstract Node addAVL(int key, int height, Node atual);

    public abstract int height(Node node);

    public abstract Node checkBalance(Node node);

    public abstract Node  find(Node node, int key);

}
